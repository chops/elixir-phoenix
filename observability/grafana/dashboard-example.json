{
  "dashboard": {
    "title": "Elixir Systems Mastery - Overview",
    "tags": ["elixir", "phoenix", "overview"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(phoenix_endpoint_stop_duration_count[5m])",
            "legendFormat": "{{method}} {{path}}"
          }
        ]
      },
      {
        "id": 2,
        "title": "Response Time (p95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(phoenix_endpoint_stop_duration_bucket[5m]))",
            "legendFormat": "p95"
          }
        ]
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(phoenix_error_rendered_total[5m])",
            "legendFormat": "{{status}}"
          }
        ]
      },
      {
        "id": 4,
        "title": "Active Processes",
        "type": "graph",
        "targets": [
          {
            "expr": "vm_process_count",
            "legendFormat": "processes"
          }
        ]
      },
      {
        "id": 5,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "vm_memory_total",
            "legendFormat": "total"
          }
        ]
      },
      {
        "id": 6,
        "title": "Database Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "ecto_db_pool_size",
            "legendFormat": "{{repo}}"
          }
        ]
      }
    ],
    "schemaVersion": 36,
    "version": 1
  }
}
