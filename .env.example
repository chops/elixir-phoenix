# FareHarbor API Configuration
# Copy this file to .env and fill in your actual credentials
# DO NOT commit the .env file to version control

# FareHarbor API Credentials
# Obtain these from your FareHarbor developer dashboard
FAREHARBOR_CLIENT_ID=your_client_id_here
FAREHARBOR_CLIENT_SECRET=your_client_secret_here

# OAuth Redirect URI (must match what you registered with FareHarbor)
FAREHARBOR_REDIRECT_URI=https://yourapp.com/callback

# Optional: Override the default API base URL
# FAREHARBOR_BASE_URL=https://fareharbor.com/api/external/v1

# For testing: Authorization code and refresh token (these are temporary)
# FAREHARBOR_AUTH_CODE=your_auth_code_here
# FAREHARBOR_REFRESH_TOKEN=your_refresh_token_here
# FAREHARBOR_COMPANY_SHORTNAME=your-company-shortname
